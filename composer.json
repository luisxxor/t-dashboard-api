{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "box/spout": "^3.0",
        "caffeinated/shinobi": "^5.0",
        "darkaonline/l5-swagger": "^6.0",
        "doctrine/dbal": "~2.3",
        "fideloper/proxy": "^4.0",
        "google/cloud-error-reporting": "^0.15.0",
        "google/cloud-logging": "^1.18",
        "google/cloud-storage": "^1.14",
        "infyomlabs/adminlte-templates": "6.0.x-dev",
        "infyomlabs/laravel-generator": "6.0.x-dev",
        "jenssegers/mongodb": "^3.6",
        "juliorafaelr/googlestorage": "*.*",
        "laravel/framework": "^6.0",
        "laravel/passport": "^7.5",
        "laravel/socialite": "^4.2",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^6.0",
        "mercadopago/dx-php": "dev-master",
        "mongodb/mongodb": "^1.4",
        "rap2hpoutre/laravel-log-viewer": "^1.3"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "mkdir bootstrap/cache",
            "php artisan cache:clear",
            "ls -la bootstrap",
            "chmod -R 755 bootstrap/cache",
        ]
    }
}
